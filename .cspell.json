{
  "allowCompoundWords": true,
  "dictionaries": ["latex", "physics", "python"],
  "enableFiletypes": [
    "git-commit",
    "github-actions-workflow",
    "julia",
    "jupyter",
    "python",
    "quarto"
  ],
  "flagWords": [
    "analyse",
    "colour",
    "comparision",
    "favour",
    "flavour",
    "hte",
    "optimise",
    "paramater",
    "parmater",
    "transision",
    "transisions"
  ],
  "ignorePaths": [
    ".cspell.json",
    ".editorconfig",
    ".envrc",
    ".gitattributes",
    ".github/",
    ".gitignore",
    ".pre-commit-config.yaml",
    ".prettierignore",
    ".vscode/",
    "*.bib",
    "*.csl",
    "*.css",
    "*.ico",
    "*.svg",
    "docs/k-matrix.jl",
    "LICENSE",
    "Manifest.toml",
    "Project.toml",
    "pyproject.toml"
  ],
  "ignoreWords": [
    "aslatex",
    "axhline",
    "axvline",
    "cbar",
    "iminuit",
    "ipynb",
    "mathbb",
    "mathrm",
    "nbsp",
    "ncols",
    "nocite",
    "pcolormesh",
    "sharey",
    "vlines",
    "vmax",
    "vmin",
    "xlabel",
    "ylabel",
    "ylim",
    "yticks"
  ],
  "language": "en-US",
  "overrides": [
    {
      "dictionaries": ["julia", "latex", "physics"],
      "filename": "**/*.jl",
      "languageId": "julia"
    }
  ],
  "useGitignore": true,
  "version": "0.2",
  "words": [
    "analyticity",
    "Anisovich",
    "Bargiotti",
    "Basdevant",
    "Breit",
    "Flatté",
    "Hüsken",
    "Ilya",
    "Indico",
    "Kallen",
    "Kopf",
    "lambdify",
    "Mikhasenko",
    "NumPy",
    "parametrizations",
    "Pixi",
    "Pöpping",
    "Remco",
    "Segal",
    "unitarization",
    "unphysical"
  ]
}
